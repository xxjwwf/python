    "    for i in range(1,n+1):\n",
    "        for k in range(i):\n",
    "            print('',end='\\t')\n",
    "        for j in range(n-i+1,0,-1):\n",
    "            print(j,end='\\t')\n",
    "        print()            \n",
    "\n",
